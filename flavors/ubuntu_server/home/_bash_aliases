alias sdtail="docker logs -f -n 10 stable-diffusion"
alias kotail="docker logs -f -n 10 kohya-ss-gui"
alias sdkill="docker restart stable-diffusion"
alias pykill="pkill python"
alias wc-nv="watch -n 1 nvidia-smi"
alias docker-stall="docker stop \$(docker ps -q)"
alias docker-clean="docker rm \$(docker ps -aq)"
alias ollama="docker exec -it ollama ollama"
